<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<databases>
    <database databaseName="Database1"/>
    <database databaseName="Database2">
        <tables>
            <table tableName="Users">
                <structure>
                    <attribute columnName="ID" type="int"/>
                    <attribute columnName="Username" type="varchar"/>
                    <attribute columnName="Email" type="varchar"/>
                </structure>
                <primaryKey/>
                <foreignKeys/>
                <IndexFiles/>
            </table>
        </tables>
    </database>
    <database databaseName="test">
        <tables>
            <table tableName="product">
                <structure>
                    <attribute columnName="productid" type="int"/>
                    <attribute columnName="productname" type="varchar(255"/>
                </structure>
                <primaryKey>
                    <pkAttribute>
                        <columnName>productid</columnName>
                    </pkAttribute>
                </primaryKey>
                <foreignKeys/>
                <IndexFiles>
                    <IndexFile>
                        <columns>ProductName</columns>
                        <indexName>idx_product_name</indexName>
                        <tableName>Product</tableName>
                    </IndexFile>
                </IndexFiles>
            </table>
        </tables>
    </database>
</databases>
